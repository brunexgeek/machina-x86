[package]
name=make
description=Make utility to maintain groups of programs
author=Michael Ringgaard
homepage=http://www.jbox.dk/sanos/

[source]
/usr/src/utils/make/

[files]
/usr/src/utils/make/
/usr/bin/make.exe

[dependencies]
cc
clib

[build]
